<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/profile_tab"
        app:startDestination="@id/profileTabFragment">

    <fragment
            android:id="@+id/profileTabFragment"
            android:name="com.example.sass.presentation.screens.tabs.profile.ProfileTabFragment"
            android:label="profile_tab_fragment"
            tools:layout="@layout/profile_tab_fragment" >
        <action
                android:id="@+id/action_profileTabFragment_to_signInFragment2"
                app:destination="@id/signInFragment2"
                app:popUpTo="@id/profileTabFragment"
                app:popUpToInclusive="true" />
    </fragment>
    <fragment
            android:id="@+id/signInFragment2"
            android:name="com.example.sass.presentation.screens.auth.SignInFragment"
            android:label="sing_in_fragment"
            tools:layout="@layout/sing_in_fragment" />
</navigation>